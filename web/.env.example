# Privy App ID - Get this from your Privy dashboard
VITE_PRIVY_APP_ID=your_privy_app_id_here

# Sepolia RPC URL - Use Infura, Alchemy, or other provider
VITE_SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_PROJECT_ID

# Factory contract address - Deploy the factory first
VITE_FACTORY_ADDRESS=0x1234567890123456789012345678901234567890

# Mock ERC20 token address (optional)
VITE_MOCK_TOKEN_ADDRESS=0x1234567890123456789012345678901234567890

# Allowed domain for signatures
VITE_ALLOWED_DOMAIN=https://dashboard.zerodev.app
